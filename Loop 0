function setup() {
  createCanvas(340, 340);
  stroke(50)
  strokeWeight(1)
}	

function draw(){
var i = 0
var x1 = 0
var y1 = 0
var x2 = 0
var y2 = 0
  for( i = 10; i < width-10; i = i + 10){
    x1 = 10
  	y1 = i
    x2 = i
    y2 = 10
    line(x1,y1,x2,y2)
  }
  for(i ; i >= 10; i = i - 10){
    x1 = width - 10
  	y1 = i
    x2 = i
    y2 = height - 10
    line(x1,y1,x2,y2)
  }
 	
}

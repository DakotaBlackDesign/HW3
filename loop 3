function setup() {
  createCanvas(340, 340);
  stroke(50)
  strokeWeight(1)
  noFill()
  rect(1,1,width-2,height-2)
}	

function draw(){
var i = 0
var x1 = 0
var y1 = 0
var x2 = 0
var y2 = 0
	for( i = 10; i <= width; i = i + 10){
    x1 = width/2
  	y1 = height/2
    d  = i
    ellipse(x1,y1,d,d)
  }
}
